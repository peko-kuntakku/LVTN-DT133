{
  "kind": "collectionType",
  "collectionName": "employees",
  "info": {
    "singularName": "employee",
    "pluralName": "employees",
    "displayName": "Employee",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Firstname": {
      "type": "string",
      "required": true
    },
    "Lastname": {
      "type": "string",
      "required": true
    },
    "Email": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "Password": {
      "type": "password",
      "required": false,
      "minLength": 6
    },
    "Phone": {
      "type": "string",
      "maxLength": 10,
      "minLength": 10
    },
    "Role": {
      "type": "integer",
      "required": true
    },
    "Salary": {
      "type": "biginteger",
      "required": true
    },
    "Sex": {
      "type": "boolean"
    },
    "MaxDayOff": {
      "type": "integer",
      "required": true
    },
    "Shift": {
      "type": "integer",
      "required": true
    },
    "Benefit": {
      "type": "biginteger"
    },
    "Grade": {
      "type": "string"
    },
    "attendances": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::attendance.attendance",
      "mappedBy": "employee"
    },
    "BirthDay": {
      "type": "date"
    },
    "WorkDays": {
      "type": "json"
    }
  }
}
