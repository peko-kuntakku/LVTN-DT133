{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Firstname": {
      "type": "string"
    },
    "Lastname": {
      "type": "string"
    },
    "Phone": {
      "type": "string"
    },
    "Relatives": {
      "type": "json"
    },
    "hadContract": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "CustomerId": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "UserInfo"
    },
    "Sex": {
      "type": "integer",
      "max": 3,
      "min": 1,
      "default": 1
    },
    "Birthday": {
      "type": "date"
    },
    "contracts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::contract.contract",
      "mappedBy": "customer"
    }
  }
}
